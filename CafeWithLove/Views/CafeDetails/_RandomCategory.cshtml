@model ICollection<CafeWithLove.Models.CafeDetail>



<li class="dropdown">
    <a href="#" class="dropdown-toggle" data-toggle="dropdown">Category <b class="caret"></b></a>
    <ul class="dropdown-menu" role="menu">
        @foreach (var item in Model)
        {
            <li>@Html.ActionLink(item.cafeCategory, "CFilter", "CafeDetails", new { chosen = item.cafeCategory }, null)</li>
        }
        
    </ul>
</li>